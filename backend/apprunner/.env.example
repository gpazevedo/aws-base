# AppRunner Service Configuration
# Copy this file to .env and update the values

# API Service URL (required)
# In AWS, this will be set automatically via environment variables
# For local development, use the API service URL
API_SERVICE_URL=http://localhost:8000

# Service configuration (optional - defaults are fine)
SERVICE_NAME=apprunner
SERVICE_VERSION=0.1.0

# HTTP client settings (optional)
HTTP_TIMEOUT=30.0
HTTP_MAX_RETRIES=3
