# API Service Configuration
# Copy this file to .env and update the values

# AppRunner Service URL (optional - for service-to-service communication)
# In AWS, this will be set automatically via environment variables
# For local development, use the AppRunner service URL
APPRUNNER_SERVICE_URL=http://localhost:8080

# Service configuration (optional - defaults are fine)
SERVICE_NAME=api
SERVICE_VERSION=0.1.0

# HTTP client settings (optional)
HTTP_TIMEOUT=30.0
