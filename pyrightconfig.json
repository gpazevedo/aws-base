{
  "include": [
    "backend/*"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.venv/lib",
    "**/.venv/bin",
    "**/node_modules"
  ],
  "venvPath": "backend",
  "executionEnvironments": [
    {
      "root": "backend/api",
      "pythonVersion": "3.14",
      "pythonPlatform": "Linux",
      "venv": "api/.venv",
      "extraPaths": [
        "../shared"
      ]
    },
    {
      "root": "backend/runner",
      "pythonVersion": "3.14",
      "pythonPlatform": "Linux",
      "venv": "runner/.venv",
      "extraPaths": [
        "../shared"
      ]
    },
    {
      "root": "backend/s3vector",
      "pythonVersion": "3.14",
      "pythonPlatform": "Linux",
      "venv": "s3vector/.venv",
      "extraPaths": [
        "../shared"
      ]
    },
    {
      "root": "backend/shared",
      "pythonVersion": "3.14",
      "pythonPlatform": "Linux",
      "venv": "shared/.venv"
    }
  ],
  "reportMissingImports": "none",
  "reportMissingTypeStubs": "warning",
  "reportMissingModuleSource": "none",
  "pythonVersion": "3.14",
  "typeCheckingMode": "basic"
}
